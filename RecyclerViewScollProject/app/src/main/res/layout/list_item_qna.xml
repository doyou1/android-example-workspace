<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="qna"
            type="com.example.recyclerviewscollproject.Qna" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="200dp">
        <TextView
            android:id="@+id/qna_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@{qna.QNA_TITLE}"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            />
        <TextView
            android:id="@+id/qna_content_dt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@{qna.QNA_CON_DT}"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/qna_title"
            />
        <TextView
            android:id="@+id/qna_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@{qna.QNA_ID}"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/qna_content_dt"
            app:layout_constraintEnd_toEndOf="parent"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>